    "paulbouwer",
    "noout",
    "ddcs",
    "citadelclient",
    "credencials",
    "showcerts",
    "eabsecret",
    "tflxs",
    "dvpj",
    "tpvht",
    "xpkp",
    "mananager",
    "overwriten",
    "GOARCH",
    "AWSPCAC",
    "myingress",
